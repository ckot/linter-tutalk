{
  "name": "linter-tutalk",
  "version": "0.1.1",
  "description": "",
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckot/linter-tutalk.git"
  },
  "homepage": {
    "url": "https://github.com/ckot/linter-tutalk#readme"
  },
  "bugs": {
    "url": "https://github.com/ckot/linter-tutalk#issues"
  },
  "configSchema": {
    "executablePath": {
      "type": "string",
      "default": "tutalk-lint",
      "description": "Full path to the tutalk-lint executable",
      "order": 1
    },
    "ignoreErrorsAndWarnings": {
      "type": "string",
      "default": "",
      "description": "comma-separated list of error and warning codes",
      "order": 2
    }
  },
  "license": "Proprietary",
  "readmeFilename": "README.md",
  "dependencies": {
    "atom-linter": "^10.0.0",
    "atom-package-deps": "^4.6.0"
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "main": "./lib/main.js"
}
